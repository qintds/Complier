var1 = 10
var2 = 3.14
var3 = "string1"
class Good {
    classVar1 = 30
    classVar2 = "string2"

    func Good(self, para1, para2) {
        self.instance1 = para1
        self.instance2 = para2
        println(self.instance1, self.instance2)
    }

    func add(self) {
        return self.instance1 + self.instance2
    }

}

func nothing() {
    var4 = 20
    instance = Good(var2, var4)
    println(instance.add())
    return instance.add(), var1
}
println(nothing())
